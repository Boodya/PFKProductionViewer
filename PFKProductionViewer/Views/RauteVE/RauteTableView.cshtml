@model BinsActual
<table class="table table-bordered">
    <thead class="thead-dark">
        <tr style="text-align: left;">
            <th scope="col">Наименование</th>
            <th scope="col">Листы (шт)</th>
            <th scope="col">Обьем (м.куб)</th>
        </tr>
    </thead>
    <tbody>
        <tr class="table-success" style="text-align: left;">
            <td>Стол 1</td>
            <td>@Model.Bin1Sheets</td>
            <td>@Model.Bin1Volume</td>
        </tr>
        <tr class="table-danger" style="text-align: left;">
            <td>Стол 2</td>
            <td>@Model.Bin2Sheets</td>
            <td>@Model.Bin2Volume</td>
        </tr>
        <tr class="table-warning" style="text-align: left;">
            <td>Стол 3</td>
            <td>@Model.Bin3Sheets</td>
            <td>@Model.Bin3Volume</td>
        </tr>
    </tbody>
</table>